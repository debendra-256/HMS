@model IEnumerable<HMSMODEL.Template>

@{
    ViewBag.Title = "Home Page";
}

<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <div class="container ">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">Hospital Management</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
            <form class="navbar-form navbar-right" role="form">
                <div class="form-group">
                    <input type="text" placeholder="Email" class="form-control">
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Password" class="form-control">
                </div>
                <button type="submit" class="btn btn-success">Sign in</button>
                <button type="submit" class="btn btn-success">Register</button>
            </form>
        </div><!--/.navbar-collapse -->
    </div>
</nav>

<div class="container col-lg-12">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
            <li data-target="#myCarousel" data-slide-to="4"></li>
            <li data-target="#myCarousel" data-slide-to="5"></li>
        </ol>
        <div class="carousel-inner">
            <div class="item active">
                <img src="http://deltamultimediaservices.com/wp-content/uploads/2014/10/Medical-Banner.jpg" alt="picture1" style="width:100%; height: 350px;">
            </div>
            <div class="item">
                <img src="https://www.altera.com/content/dam/altera-www/global/en_US/images/altera-new/feature-medical-450x235.png" alt="picture2" style="width: 100%; height: 350px;">
            </div>
            <div class="item">
                <img src="https://previews.123rf.com/images/kurhan/kurhan1503/kurhan150300179/37141939-Medical-physician-doctor-hands-Healthcare-banner--Stock-Photo.jpg" alt="picture3" style="width: 100%; height: 350px;">
            </div>
            <div class="item">
                <img src="http://www.atlascoegypt.com/images/products/Medical/251Medical-Devices.jpg" alt="picture4" style="width: 100%; height: 350px;">
            </div>
            <div class="item">
                <img src="https://www.owu.edu/files/pages/medical-and-health-2.jpg" alt="picture5" style="width: 100%; height: 350px;">
            </div>
            <div class="item">
                <img src="https://www.athona.com/wp-content/uploads/2014/03/banner-medicine-large.jpg" alt="picture5" style="width: 100%; height: 350px;">
            </div>
            <div class="item">
                <img src="https://www.athona.com/wp-content/uploads/2014/03/banner-medicine-large.jpg" alt="picture5" style="width: 100%; height: 350px;">
            </div>

        </div>
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>





